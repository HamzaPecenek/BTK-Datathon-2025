    "# Cell 8\n",
    "pipe.fit(train_sess[cat_features + num_features], np.log1p(train_sess[target].values))\n",
    "test_pred = np.expm1(pipe.predict(test_sess[cat_features + num_features])).clip(min=0)\n",
    "\n",
